{
  "name": "dreamkas",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "history": "^4.6.3",
    "immutable": "^3.8.1",
    "material-ui": "^0.18.7",
    "node-sass-chokidar": "0.0.3",
    "normalize.css": "^7.0.0",
    "npm-run-all": "^4.0.2",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-scripts": "1.0.10",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src src/ -o src/ --watch --recursive",
    "start-js": "cross-env NODE_PATH=src/ PORT=3006 react-scripts start",
    "build-js": "cross-env NODE_PATH=src/ react-scripts build",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && npm run build-js"
  },
  "devDependencies": {
    "cross-env": "^5.0.1",
    "whatwg-fetch": "^2.0.3"
  }
}
